{
  "expo": {
    "name": "life-simulation-game",
    "slug": "life-simulation-game",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "newArchEnabled": false,
    "runtimeVersion": "54.0.0",
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false
          }
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lifesimulationgame.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.lifesimulationgame.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "platforms": ["ios", "android", "web"],
    "scheme": "lifesimulationgame"
  }
}
